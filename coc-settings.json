{
	"sumneko-lua.locale": "zh-cn",
	"volar.tsLocale": "zh-cn",
	"tsserver.locale": "zh-CN",
	"codeLens.enable": true,
	"codeLens.position": "right_align",
	"colors.enable": true,
	"semanticTokens.enable": true,
	"coc.preferences.enableMessageDialog": true,
	"coc.preferences.formatOnSave": true,
	"coc.preferences.formatOnType": true,
	"extensions.updateCheck": "daily",
	"coc.preferences.messageLevel": "error",
	"suggest.labelMaxLength": 65,
	"suggest.localityBonus": true,
	"suggest.removeCurrentWord": true,
	"suggest.noselect": true,
	"suggest.enablePreselect": false,
	"suggest.filterGraceful": true,
	"suggest.insertMode": "insert",
	"suggest.triggerAfterInsertEnter": true,
	"suggest.autoTrigger": "always",
	"suggest.defaultSortMethod": "alphabetical",
	"suggest.detailField": "abbr",
	"suggest.minTriggerInputLength": 2,
	"suggest.timeout": 600,
	"diagnostic.floatConfig": {
		"border": true,
		"rounded": true,
		"highlight": "CocFloating",
		"focusable": false,
		"title": "🔐"
	},
	"hover.previewMaxHeight": 8,
	"hover.floatConfig": {
		"border": true,
		"rounded": true,
		"title": "📚",
		"highlight": "CocFloating"
	},
	"suggest.floatConfig": {
		"border": true,
		"rounded": true,
		"highlight": "CocFloating"
	},
	"suggest.pumFloatConfig": {
		"border": true,
		"rounded": true,
		"highlight": "CocFloating"
	},
	"signature.floatConfig": {
		"border": true,
		"rounded": true,
		"highlight": "CocFloating",
		"focusable": false,
		"title": "🏷️"
	},
	"floatFactory.floatConfig": {
		"border": true,
		"rounded": true,
		"highlight": "CocFloating",
		"focusable": false,
		"title": "🫰"
	},
	"notification.winblend": 0,
	"suggest.virtualText": true,
	"suggest.removeDuplicateItems": true,
	"suggest.reversePumAboveCursor": true,
	"suggest.snippetIndicator": " 󰁨 ",
	"suggest.selection": "recentlyUsed",
	"suggest.triggerCompletionWait": 0,
	"suggest.completionItemKindLabels": {
		"text": "󰓽 Text",
		"method": "m Method",
		"function": "󰊕 Func",
		"constructor": " Constructor",
		"field": " Field",
		"variable": "󱄑 Variable",
		"class": " Class",
		"interface": "󱇰 Interface",
		"module": " Module",
		"property": "󰀔 Property",
		"unit": " Unit",
		"value": "󱀍 Value",
		"keyword": "󰌆 Keyword",
		"snippet": " Snippet",
		"color": " Color",
		"file": " File",
		"reference": " Reference",
		"folder": " Folder",
		"enum": " Enummember",
		"constant": " Constant",
		"struct": "  Struct",
		"event": " Event",
		"operator": " Operator",
		"typeParameter": "󰉺 TypeParameter",
		"default": "󰊨 default"
	},
	"suggest.formatItems": ["abbr", "menu", "kind", "shortcut"],
	"diagnostic.errorSign": "\uf467",
	"diagnostic.warningSign": "\uf071",
	"diagnostic.infoSign": "\uf129",
	"diagnostic.hintSign": "\uf864",
	"diagnostic.displayByAle": false,
	"diagnostic.refreshOnInsertMode": false,
	"diagnostic.checkCurrentLine": true,
	"diagnostic.virtualTextPrefix": "↪  ",
	"diagnostic.virtualTextCurrentLineOnly": false,
	"diagnostic.virtualText": true,
	"scss.validate": true,
	"diagnostic.enableMessage": "jump",
	"session.saveOnVimLeave": false,
	"list.previewHighlightGroup": "Statement",
	"list.source.files.excludePatterns": ["target/.*", "**/.git/**", "**/node_modules/**", "dist"],
	"list.nextKeymap": "<tab>",
	"list.previousKeymap": "<S-tab>",
	"list.normalMappings": {
		"<Tab>": "do:next",
		"<S-Tab>": "do:previous"
	},
	"list.floatPreview": true,
	// "list.previewSplitRight": true,
	"list.statusLineSegments": [],
	"list.alignColumns": true,
	"list.indicator": "coc-😀",
	"snippets.ultisnips.directories": [
		"$HOME/.config/nvim/Ultisnips/",
		"$HOME/.config/nvim/plugged/vim-snippets/UltiSnips/"
	],
	"yaml.format.enable": true,
	"signature.target": "float",
	"yank.enableCompletion": false,
	"jest.watch": false,
	"flutter.openDevLogSplitCommand": "botright 12split",
	"dart.lineLength": 100,
	"html.format.enable": true,
	"git.addedSign.hlGroup": "GitGutterAdd",
	"git.changedSign.hlGroup": "GitGutterChange",
	"git.removedSign.hlGroup": "GitGutterDelete",
	"git.topRemovedSign.hlGroup": "GitGutterDelete",
	"git.changeRemovedSign.hlGroup": "GitGutterDelete",
	"git.changedSign.text": "┃",
	"git.addedSign.text": "┃",
	"git.removedSign.text": "▁",
	"git.topRemovedSign.text": "▔",
	"git.changeRemovedSign.text": "≃",
	"git.branchCharacter": "",
	"git.showCommitInFloating": true,
	"git.addGBlameToVirtualText": false,
	"git.addGBlameToBufferVar": true,
	"git.splitWindowCommand": "below 10sp",
	"javascript.showUnused": true,
	"javascript.suggest.names": true,
	"javascript.suggestionActions.enabled": true,
	"json.format.enable": true,
	"prettier.printWidth": 100,
	"prettier.disableLanguages": [],
	"prettier.formatterPriority": 1,
	"prettier.useTabs": true,
	"prettier.trailingComma": "all",
	"prettier.singleQuote": false,
	"emmet.includeLanguages": {
		"javascript": "javascriptreact",
		"vue-html": "html",
		"razor": "html",
		"plaintext": "jade"
	},
	"diagnostic-languageserver.formatFiletypes": {
		"elixir": "mix_format",
		"eelixir": "mix_format"
	},
	"snippets.ultisnips.pythonPrompt": false,
	"snippet.statusText": "😉",
	"symbol-line.separator": " ",
	"coc.source.emoji.triggerCharacters": [";"],
	"coc.source.emoji.filetypes": null,
	"inlayHint.enableParameter": true,
	"inlayHint.enable": true,
	"vue.inlayHints.inlineHandlerLeading": false,
	"vue.inlayHints.missingProps": true,
	"typescript.inlayHints.parameterNames.enabled": "all",
	"typescript.suggestionActions.enabled": true,
	"typescript.inlayHints.parameterTypes.enabled": true,
	"typescript.implementationsCodeLens.enabled": true,
	"typescript.referencesCodeLens.enabled": true,
	"inlayHint.refreshOnInsertMode": false,
	"languageserver": {
		"golang": {
			"command": "gopls",
			"rootPatterns": ["go.mod"],
			"filetypes": ["go"],
			"initializationOptions": {
				"hints": {
					"assignVariableTypes": true, // 显示变量类型提示
					"compositeLiteralFields": true, // 结构体字段名提示
					"constantValues": true, // 常量值提示
					"functionTypeParameters": true, // 函数类型参数提示
					"parameterNames": true, // 参数名提示
					"rangeVariableTypes": true // range 循环变量类型提示
				}
			}
		}
	},
	"go.goplsOptions": {
		"gofumpt": true,
		"hints": {
			"assignVariableTypes": true,
			"compositeLiteralFields": true,
			"compositeLiteralTypes": true,
			"constantValues": true,
			"functionTypeParameters": true,
			"parameterNames": true,
			"rangeVariableTypes": true
		}
	},
	"coc.source.around.shortcut": "AR",
	"coc.source.word.shortcut": "WD",
	"coc.source.around.priority": 50,
	"coc.source.word.priority": 50,
	"coc.source.emoji.priority": 49,
	"snippets.priority": 100,
	"coc.source.emoji.shortcut": "EO",
	"snippets.shortcut": "SN",
	"coc.source.buffer.shortcut": "BF",
	"coc.source.file.shortcut": "FL"
}
